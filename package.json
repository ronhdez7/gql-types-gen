    "gen",
    "types"
  ],
  "author": "ronhdez7",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.20",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "commander": "^12.0.0"
  }
}
